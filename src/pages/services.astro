	<CardBox cardData={devCard} />
	<FloatBox floatBoxData={coursesFloat}/>
	<SectionHeader sectionHeader='Tableau para todos' />
	<CardBox cardData={tableauCard1} />
	<CardBox cardData={tableauCard2} />
	<WideBox wideBoxData={tableauWide}/>
</BaseLayout>
